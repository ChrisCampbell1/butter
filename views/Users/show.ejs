<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>

<main>
<img src="<%= profile.avatar %>" alt="user avatar">
<h1>Recipes by <%= profile.name %>:</h1>
<ul>
  <% recipes.forEach(recipe => { %>
    <li>
      <a href="/recipes/<%= recipe._id %>"><%= recipe.label %></a>
    </li>
  <% }) %>
</ul>
</main>

<%- include('../partials/footer') %>
